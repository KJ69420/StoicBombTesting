    v1407.UPPERCUTSMOKE["2"].Color = ColorSequence.new(l_Color_0)
    for _, child in ipairs(v1407:GetChildren()) do
        if child:IsA("ParticleEmitter") then
            child:Emit(6)
        end
    end
    game:GetService("Debris"):AddItem(v1407, 2)
end

