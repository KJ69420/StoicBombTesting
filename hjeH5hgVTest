task.spawn(function()
wait(2.3)

-- Define the Lighting service and TweenService
local lighting = game:GetService("Lighting")
local tweenService = game:GetService("TweenService")

-- Set initial exposure (brightness) level
lighting.ExposureCompensation = 0 -- Starting from normal brightness

-- Define the goal to increase the exposure (or brightness)
local tweenInfo = TweenInfo.new(
    0.8, -- Time in seconds for the transition
    Enum.EasingStyle.Quad, -- Easing style
    Enum.EasingDirection.Out -- Easing direction
)

-- Set the target brightness (higher ExposureCompensation means brighter)
local goal = {ExposureCompensation = 10}

-- Create and play the tween
local tween = tweenService:Create(lighting, tweenInfo, goal)
tween:Play()

-- Reset the exposure back to normal once the tween completes
tween.Completed:Connect(function()
    -- Define the reset tween
    local resetTweenInfo = TweenInfo.new(
        0, -- Time in seconds for the reset
        Enum.EasingStyle.Quad, -- Easing style
        Enum.EasingDirection.InOut -- Easing direction
    )
    
    local resetGoal = {ExposureCompensation = 0} -- Reset back to normal brightness
    
